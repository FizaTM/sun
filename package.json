  "keywords": [
    "language"
  ],
  "author": "mavenave",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.1.1",
    "gulp": "^3.9.1",
    "tap": "^8.0.1",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {
    "debug": "^2.6.0",
    "jison": "^0.4.17",
    "readline-sync": "^1.4.5"
  }
}
