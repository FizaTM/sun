{
  "name": "jlang",
  "version": "1.0.0",
  "description": "Implementation of dr juan's pseudocode language",
  "main": "index.js",
  "scripts": {
    "test": "tap test/*.js",
    "test-cov": "tap test/*.js --cov --coverage-report=html --100",
    "build": "browserify src/sun.js -s SunCompiler -i readline-sync | uglifyjs > docs/js/sun.js",
    "serve": "jekyll serve -s docs"
